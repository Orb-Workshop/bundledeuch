{
    "name": "bundledeuch",
    "module": "scripts/main.js",
    "type": "module",
    "private": true,
    "dependencies": {
        "scriptedeuch": "github:Orb-Workshop/scriptedeuch"
    },
    "devDependencies": {
        "@types/bun": "latest"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "scripts": {
        "build": "bun build ./src/main.ts --outdir ./scripts --external cs_script/point_script",
        "watch": "bun --watch build ./src/main.ts --outdir ./scripts --external cs_script/point_script"
    },
    "bun-create": {
        "start": "bun run setup.ts"
    }
}
